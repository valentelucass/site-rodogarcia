{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/",
      "dest": "/src/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/src/index.html"
    },
    {
      "src": "/servicos.html",
      "dest": "/src/servicos.html"
    },
    {
      "src": "/sobre.html",
      "dest": "/src/sobre.html"
    },
    {
      "src": "/cotacao.html",
      "dest": "/src/cotacao.html"
    },
    {
      "src": "/trabalhe-conosco.html",
      "dest": "/src/trabalhe-conosco.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
