{
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/src/index.html"
    },
    {
      "source": "/servicos.html",
      "destination": "/src/servicos.html"
    },
    {
      "source": "/sobre.html",
      "destination": "/src/sobre.html"
    },
    {
      "source": "/cotacao.html",
      "destination": "/src/cotacao.html"
    },
    {
      "source": "/trabalhe-conosco.html",
      "destination": "/src/trabalhe-conosco.html"
    },
    {
      "source": "/css/(.*)",
      "destination": "/src/css/$1"
    },
    {
      "source": "/script/(.*)",
      "destination": "/src/script/$1"
    },
    {
      "source": "/components/(.*)",
      "destination": "/src/components/$1"
    },
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    }
  ]
}
